<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
            {# <link rel="stylesheet" href="../assets/css/grid.css"> #}
            <link rel="stylesheet" href="/../assets/css/style.css">
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.3.0/ekko-lightbox.min.js">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
    <style>
        body{
            padding-top:100px;
        }
        .header{
            height: 400px;
            background-image:url("{{ asset('images/FOND-04.jpg') }}");
        }

        .email{
            position: absolute;
            left: -50px; top: 200px; 
            transform: rotate(-90deg);
        }
        .contact{
            background-image:url("{{ asset('images/fond-contact.svg') }}");
        }
        .background_head{
            width:100%;
            height:100px;
            background:var(--color-violet);
            box-shadow:0 1px 30px rgb(33, 33, 33);
        }
        .overlay{
            margin-top:100px;
        }


    </style>
        <header  id="header" class="header">
                <div class="background_head fixed-top">
                    <div class="navbar fixed-top container">
                        
                        <!-- réseaux gauche -->
                        <a href="{{ path('homepage') }}" class="logo-header"><img src='{{ asset('images/logo-header.svg') }}' alt=""></a>
                        <ul class="reseaux-sociaux">
                            <li><a href="https://www.facebook.com/cleo.csn"target="_blank"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="https://www.instagram.com/cleo__cook/?hl=fr"target="_blank"><i class="fa fa-instagram"></i></a></li>
                            <li><a href="https://www.linkedin.com/in/cl%C3%A9o-cosnier-092b7668"target="_blank"><i class="fa fa-linkedin"></i></a></li>
                            <!-- <li><a href=""><i class="fa fa-behance"></i></a></li> -->
                            <li><a href="https://www.pinterest.fr/cleocosnier/"><i class="fa fa-pinterest-p"target="_blank"></i></a></li>
                        </ul>
                        
                        <!-- menu à droite -->
                        
                        <ul class="menu">
                            <li><a href="{{ path('about') }}">À PROPOS</a></li>
                            <li><a href="{{ path('prestations') }}">PRESTATIONS</a></li>
                            <li><a href="{{ path('portfolio') }}">PORTFOLIO</a></li>
                            <li><a href="{{ path('blog') }}">BLOG</a></li>
                            <li><a href="{{ path('homepage') }}#contact">CONTACT</a></li>
                            <span onclick="openNav()"><i class="fa fa-bars mt-0"></i></span>
                        </ul>
                    </div>
                </div>

                <div class="header-title d-flex align-items-center container">
                    <div class="header_content_titre">
                        <h1>{{name}}</h1>
                        <div class="filet"></div>
                        
                    </div>
                </div>
                  
                <!-- <div class="fond-menu-scroll col-12"></div> -->
                <div class="email">
                    <a href="mailto:cleocosnier@gmail.com">cleo.cosnier@gmail.com</a>
                </div>
                
                
                <!-- The overlay -->
                <div id="myNav" class="overlay">

                    <!-- Button to close the overlay navigation -->
                    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
                
                    <!-- Overlay content -->
                    <div class="overlay-content">
                    <a href="{{ path('homepage') }}">HOME</a>
                    <a href="{{ path('about') }}">À PROPOS</a>
                    <a href="{{ path('prestations') }}">PRESTATIONS</a>
                    <a href="{{ path('portfolio') }}">PORTFOLIO</a>
                    <a href="{{ path('blog') }}">BLOG</a>
                    <a href="{{ path('homepage') }}#contact">CONTACT</a>
                    </div>
                
                </div>
         
                
 
            
        </header>

    
        {% block body %}{% endblock %}
        {% block contact %}
            <section class="contact">
                <div class="contact-content container">
                    <div class="contact-text">
                        <h2>À VOTRE ÉCOUTE</h2>
                        <div class="filet"></div>
                    </div>
                    <a href="{{ path('homepage') }}#contact" class="btn btn-primary btn-4 btn-4c icon-arrow-right">CONTACT</a>
                </div>
            </section>
        {% endblock %}
        {% block footer %}
        {# scroll to top #}

        <a href="#header" class="back-to-top">
                <i class='fa fa-angle-double-up'></i>
            </a>
            <footer class="footer">
                    <img src='{{ asset('images/logo-footer.svg') }}'alt="">
                    <ul class="menu">
                        <li><a href="{{ path('about') }}">À PROPOS</a></li>
                        <li><a href="{{ path('prestations') }}">PRESTATIONS</a></li>
                        <li><a href="{{ path('portfolio') }}">PORTFOLIO</a></li>
                        <li><a href="{{ path('blog') }}">BLOG</a></li>
                        <li><a href="{{ path('homepage') }}#contact">CONTACT</a></li>
                        <li><a href="{{ path('terms_of_use') }}">MENTIONS LÉGALES</a></li>
                    </ul>
                </footer>
        {% endblock %}

        <script src="../assets/app.js"></script>
        <script src="../assets/social_share.js"></script>
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        <script src="{{ asset('assets/ckeditor/ckeditor.js') }}" ></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
