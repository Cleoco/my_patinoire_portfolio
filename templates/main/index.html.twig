    {# <!-- ----------------------------COMPETENCES------------------------------ --> #}


<section class="competences">
    <div class="titre">
        <h2>COMPÉTENCES</h2>
        <div class="filet"></div>
    </div>
               
        <div class="container row cards">
        {% set index = 0 %}
            {% for category in categories %}
                <!-- filtres -->
                    <div class="card col-lg-3 col-md-4 col-sm-12" data-index={{ index }}>
                        <img src='{{ asset('images/category/' ~ category.imageName ~ '') }}' alt="">
