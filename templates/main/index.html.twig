                <a href="{{ path('projet_show', {'id': projet.id}) }}"><img src="{{ asset('images/projet/' ~ projet.imageUneName ~ '') }}" alt="">
                <figcaption>
                    <h3>{{ projet.title }}</h3>
                    <div class="filet-fin"></div>
                    {# <p>{{ projet.category }}</p> #}
                </figcaption>
                </a>
            </figure>
        </div>
    {% endfor %}
    
      
        <a href="{{ path('portfolio') }}" class="btn btn-primary btn-4 btn-4c icon-arrow-right">voir tous les projets</a>
    </div>
</section>


