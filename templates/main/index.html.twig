                        <div class="filet-fin"></div>
                    </div>
                    {% set index = index + 1 %}
            {% endfor %}
            <a href="#contact" class="card col-lg-3 col-md-4 col-sm-12">
                <img class='img-tel'src="{{ asset('images/Groupe54.svg') }}" alt="">
