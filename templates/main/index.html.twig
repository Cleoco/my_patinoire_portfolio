<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
            {# <link rel="stylesheet" href="../assets/css/grid.css"> #}
            <link rel="stylesheet" href="/../assets/css/style.css">
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.3.0/ekko-lightbox.min.js">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>



{% block body %}

<style>

/* <!-- ----------------------------HEADER------------------------------ --> */

.header{
    background-image:url("{{ asset('images/Logo-fond-V2-02 copy.jpg') }}");
    height: 100vh;
}

.header_content_all {
    position: relative;
}

.header_content{
    height: auto;
    display: flex;
    align-items: flex-end;
}

.header_content_titre{
    max-height: 100vh;
    margin-bottom: 30px;
    position: absolute;
    bottom: 0;
}

.header_content_titre p {
    line-height: 50px;
    letter-spacing: 0.2rem;
}

.header_content_titre h1{
    font-family: 'Montserrat',sans-serif;
    letter-spacing: 0.5rem;
    font-size: 50px;
    font-weight: 600;
}


.header_content_titre img{
    margin-top: 50px;
    animation: bounce 0.35s ease infinite alternate; 
} 
@keyframes bounce{ 
    from {transform: translateY(0);} 
    to {transform: translateY(-5px);}
} 

.email{
    position: absolute;
    left: -50px; top:50vh;
    transform: rotate(-90deg);
}

  
@media(min-width:768px){
    .fa-bars{visibility: hidden;}

}

@media (max-width:768px){

    .header{
        background-image:url(../css/images/Logo-fond-V2-02.jpg);
    }
    .header_content_titre p {
       visibility: hidden;
    }
    .header_content_titre h1{
        font-family: 'Montserrat',sans-serif;
        letter-spacing: 0.5rem;
        font-size: 35px;
        font-weight: 600;
    }
}

/* <!-- ----------------------------ABOUT------------------------------ --> */


.about{
    padding-top: 140px;
    padding-bottom: 140px;
    position: relative;
    background-image:url(../css/images/graphic-about.svg);
    width: 100%;
    background-position: center top;
    background-repeat:no-repeat;

}

.rect-haut-prez{
    position: absolute;
    top: -3px; right: 218px;
}

.rect-bas-prez{
    position: absolute;
    top: 557px; right: 450px;
}


@media (max-width:992px){
    .filet{
        margin-left: auto;
        margin-right: auto;
    }
    .about_content_titre{
        text-align: center;
    }
    .about_content_titre .btn{
        margin-bottom: 30px;
       
    }
}
  
@media(min-width:768px){
    .about_content_titre .btn{
        margin-bottom: 30px;
       
    }

}

@media (max-width:768px){

    
}


/* <!-- ----------------------------COMPETENCES------------------------------ --> */


.competences{
    background-image:url(../css/images/graphic-competences.svg);
    width: 100%;
    background-position: center bottom;
    background-repeat:no-repeat;
    background-color: var(--color-grey);
    height: auto;
    padding-top: 106px;
    padding-bottom: 106px;
}
.titre{
    text-align: center;
}
.competences .filet{
    margin: 40px auto;
}
.titre h2{
    color: var(--color-dark-grey);
}

.card{
    width: 268px;
    height: auto;
    background: white;
    box-shadow: 0px 0px 30px rgb(190, 190, 190);
    border-radius: 20px;
    margin: 20px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 50px;
    position: relative;
    text-decoration: none;
}

.cards {
    margin: 0 auto;
    display: flex;
    justify-content: center;
    

}

.card:nth-child(6){
    background: var(--color-violet);
}
.card:nth-child(6):hover{
    background: var(--color-blue);
    transition: 0.2s;
}
.img-tel{
    margin-bottom: 30px;
}
.card img{
    margin-bottom: 30px;
    width:80px !important;
    height:80px !important;
    
}

.img-neutre {
    margin: 0 auto;
    fill: var(--color-violet);
 }

.card span{
    color: white;
}

.competences .btn{
    margin-top: 50px;
}


@media (max-width:992px){

}
  
@media(min-width:768px){


}

@media (max-width:768px){

    .competences .btn{
        text-align: center;
        margin-left: 50px;
        margin-right: 50px;
        border-radius: 50px;
    }
    .card:nth-child(6):hover{
        background: var(--color-violet);
    }
}



/* <!-- ----------------------------PORTFOLIO------------------------------ --> */

.portfolio{
    padding-top: 106px;
    padding-bottom: 106px;
    background-image:url(../css/images/graphic-portfolio.svg);
    width: 100%;
    background-position: center bottom;
    background-repeat:no-repeat;
    height: auto;
}
.portfolio .filet{
    margin: 40px auto;
}

.gallery{
    box-sizing: border-box;
}

.gallery .image{
    /* width: 455px; */
    height: auto;
    margin: 0;
    padding: 0;
    
}
figure img{
    
    width: 100%;
    height: auto;
    object-fit: cover;
    display: table;
}
.gallery .btn{

    margin-top: 50px;
    margin-left: auto;
    margin-right: auto;
}

figure{
    position: relative;
    overflow: hidden;
    margin: 0;
    padding: 0;
    height:100%;
    width:100%;
}

.image figcaption{
    display: flex;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color:var(--color-violet);
    opacity: 0;
    transition: .4s; 
    color: white;
    flex-direction: column;
    padding-left: 70px;
    justify-content: center;
}

.image figcaption:hover{
    opacity:0.9;
}

.image figcaption h3{
    color: white;
    text-align: left;
}

.image figcaption .filet-fin{
    background: white;
    margin-left: 0;
    margin-bottom: 30px;
}

.btn-figcaption{

    background: white;
    padding-left: 10px;
    padding-right: 10px;
    align-self: flex-end;
    margin-right: 70px; 
    background: white;
    border-radius: 30px;
    text-decoration: none;
    color: white;
    font-family: 'Montserrat', sans-serif;
    font-size: 14px;
    text-transform: uppercase;
	cursor: pointer;
	display: inline-block;
	padding: 15px 20px;
}

@media (max-width:992px){

}
  
@media(min-width:768px){
  

}

@media (max-width:768px){

    .btn-figcaption{
        margin-right: 50px;
    }
    figcaption{
        visibility: hidden;
    }
    .gallery .btn{
    text-align: center; 
    }

}



/* <!-- ----------------------------CONTACT------------------------------ --> */

.contact {
    height: auto;
    padding-top: 106px;
    padding-bottom: 106px;
    background: var(--color-violet);
    background-image:url(../css/images/graphic-contact.svg);
    width: 100vw;
    background-position: center bottom;
    background-repeat:no-repeat;
    height: auto;
}
.contact_content-titre h2{
    color: white;
}
.contact_content-titre .filet{
    background: white;
}
.coordonnees {
    color: white;
    margin-top: 50px;
    
}
.coordonnees p{
    margin-left: 30px;
}
.coordonnees img{
    width: 20px;
}
.telephone{
    display: flex;
}
.telephone p{
    font-size: 20px;
    font-weight: 600;
    letter-spacing: 0.2rem;
}
.adresse{
    margin-top: 20px;
    display: flex;
}
.reseaux h3{
    text-align: left;
    color: white;
    padding: 30px 0;
}
.reseaux-sociaux .fa{
    color: white;
}

.form-control{
    height: 40px;
    width: 250px;
    width: auto;
    border-radius: 30px;
    border: 2px solid white;
    padding-left: 30px;
    padding-right: 30px;
    font-size: 12px;
    letter-spacing: 2px;
    margin-bottom:30px;
}
.form-control:hover {
    box-shadow: 5px 5px 10px rgb(54, 54, 155);
}
 
form{
    display: flex;
    flex-direction: column;
    padding-left:70px;
}
.form-name{
    display: flex;
    justify-content: space-between;
}
.form-name>*{
    width: 320px;
}

.message-form{
    height: 100px;
    padding-top:15px;
}

.btn-form{
    width: 200px;
    padding: 10px 30px;
    background: var(--color-blue);
    border: 0px;
    align-self: flex-end;
}

.btn-form:hover{

    background: var(--color-footer);
    transition: 0.3s;
}



@media(max-width:576px){

}

@media(max-width:768px){
    .form-contact .btn{
        width: 100%;
    }
}
@media(min-width:576px){
    .contact{
        text-align: center;
    }
    .telephone{
        justify-content: center;
    }
    .adresse {
        justify-content: center;
    }

    .form-contact{
        margin-top: 50px;
        padding-left: 15px;
    }

    .reseaux h3{
        text-align: center;
    }
    .reseaux-sociaux{
        justify-content: center;
    }
    .btn-form{
        width: 100%;
        padding: 10px 30px;
        background: var(--color-blue);
        border: 0px;
        align-self: flex-end;
    }
    
    .contact_content-titre{
        margin-bottom: 30px;
    }

    .contact_content-titre .filet{
        margin-left: auto;
        margin-right: auto;
    }

    form{
        padding-left:15px;
    }
    .form-control{
        width: 100%;
    }
 

}

</style>

